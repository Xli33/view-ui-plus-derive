{
  "name": "view-ui-plus-derive",
  "version": "0.0.2",
  "type": "module",
  "description": "基于view-ui-plus的Vue3组件等扩展",
  "keywords": [
    "view-ui-plus-derive",
    "component",
    "vuejs",
    "vue",
    "component-library",
    "ui",
    "typescript",
    "derive"
  ],
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./locale/*": {
      "types": "./dist/locale/index.d.ts",
      "import": "./dist/locale/*.js"
    },
    "./iview-mods/*": {
      "types": "./dist/iview-mods/index.d.ts",
      "import": "./dist/iview-mods/*.js"
    },
    "./iview-mod": "./dist/iview-mod.js",
    "./iview-mod.css": "./dist/iview-mod.css",
    "./style": "./dist/style.js",
    "./styles/*": "./dist/styles/*"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "prepare": "husky",
    "release": "commit-and-tag-version",
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "test:unit": "vitest",
    "build-only": "vite build",
    "type-check": "vue-tsc --build",
    "lint": "eslint . --fix",
    "format": "prettier --write src/",
    "prepublishOnly": "npm run build && npm run release"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Xli33/view-ui-plus-derive.git"
  },
  "author": "Xli33",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Xli33/view-ui-plus-derive/issues"
  },
  "homepage": "https://github.com/Xli33/view-ui-plus-derive#readme",
  "peerDependencies": {
    "dayjs": "^1.11.13",
    "utils-where": "^0.3.7"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@tsconfig/node22": "^22.0.2",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^24.3.0",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vitejs/plugin-vue-jsx": "^5.1.0",
    "@vitest/eslint-plugin": "1.3.4",
    "@vue/eslint-config-prettier": "^10.1.0",
    "@vue/eslint-config-typescript": "^14.6.0",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.8.1",
    "commit-and-tag-version": "^12.5.2",
    "dayjs": "^1.11.13",
    "eslint": "^9.34.0",
    "eslint-plugin-vue": "^10.4.0",
    "husky": "^9.1.7",
    "jiti": "^2.5.1",
    "jsdom": "^26.1.0",
    "less": "^4.4.1",
    "npm-run-all2": "^8.0.4",
    "prettier": "^3.6.2",
    "typescript": "~5.9.2",
    "utils-where": "^0.3.7",
    "view-ui-plus": "^1.3.20",
    "vite": "^7.1.3",
    "vite-plugin-dts": "^4.5.4",
    "vite-plugin-vue-devtools": "^8.0.1",
    "vitest": "^3.2.4",
    "vue": "^3.5.20",
    "vue-i18n": "^11.1.11",
    "vue-tsc": "^3.0.6"
  }
}
